<!-- map.wxml -->
<map
  id="map"
  longitude="113.324520"
  latitude="23.099994"
  enable-zoom="true"
  scale="14"
  controls="{{controls}}"
  bindcontroltap="controltap"
  markers="{{markers}}"
  bindmarkertap="markertap"
  polyline="{{polyline}}"
  bindregionchange="regionchange"
  show-location
  style="width: 100%; height: 1000rpx;"
></map>